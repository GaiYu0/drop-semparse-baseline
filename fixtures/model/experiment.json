{
  "dataset_reader": {
    "type": "drop",
    "tables_directory": "fixtures/data/",
    "offline_logical_forms_directory": "fixtures/data/",
    "embedding_file_for_entity_extraction": "fixtures/data/embedding_file.txt.gz"
  },
  "train_data_path": "fixtures/data/sample_data.examples",
  "validation_data_path": "fixtures/data/sample_data.examples",
  "model": {
    "type": "drop_mml_parser",
    "question_embedder": {
      "tokens": {
        "type": "embedding",
        "embedding_dim": 25,
        "trainable": true
      }
    },
    "action_embedding_dim": 50,
    "encoder": {
      "type": "lstm",
      "input_size": 50,
      "hidden_size": 10,
      "num_layers": 1
    },
    "entity_encoder": {
      "type": "boe",
      "embedding_dim": 25,
      "averaged": true
    },
    "decoder_beam_search": {
      "beam_size": 3
    },
    "max_decoding_steps": 200,
    "attention": {"type": "dot_product"}
  },
  "iterator": {
    "type": "bucket",
    "sorting_keys": [["question", "num_tokens"]],
    "padding_noise": 0.0,
    "batch_size" : 2
  },
  "trainer": {
    "num_epochs": 2,
    "patience": 10,
    "cuda_device": -1,
    "optimizer": {
      "type": "sgd",
      "lr": 0.01
    }
  }
}
